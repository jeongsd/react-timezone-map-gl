{
  "name": "react-timezone-map-gl",
  "version": "1.0.0",
  "description": "react-timezone-map-gl React component",
  "author": "jeongsd",
  "license": "MIT",
  "repository": "jeongsd/react-timezone-map-gl",
  "main": "dist/TimezoneMapGL.js",
  "module": "dist/TimezoneMapGL.modern.js",
  "source": "src/TimezoneMapGL.tsx",
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": "./dist/foo.modern.js"
  },
  "scripts": {
    "build": "microbundle-crl --compress --format modern,cjs",
    "start": "run-p start:dev copy:watch",
    "start:dev": "microbundle-crl watch --compress --format modern,cjs",
    "copy": "cpx 'src/shared/**' 'src/legacy/shared/'",
    "copy:watch": "cpx 'src/data/**' 'dist'",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "generateTimezoneData": "scripts/generateTimezoneData.sh"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@danmarshall/deckgl-typings": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/luxon": "^1.25.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-map-gl": "^5.2.9",
    "@types/topojson-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "mapshaper": "^0.5.30",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup-plugin-copy-assets": "^2.0.3",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@react-hook/debounce": "^3.0.0",
    "luxon": "^1.25.0",
    "react-map-gl": "^6.0.3",
    "timezone-support": "^2.0.2",
    "topojson-client": "^3.1.0"
  }
}
